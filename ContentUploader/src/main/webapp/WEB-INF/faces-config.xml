<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<managed-bean>
		<managed-bean-name>clientScriptBackingBean</managed-bean-name>
		<managed-bean-class>AIR.Common.Web.taglib.ClientScriptContainerBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>contentUploadBacking</managed-bean-name>
		<managed-bean-class>tds.ContentUploader.Web.backing.ContentUploadBacking</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>contentPublishBacking</managed-bean-name>
		<managed-bean-class>tds.ContentUploader.Web.backing.ContentPublishBacking</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
</faces-config>
